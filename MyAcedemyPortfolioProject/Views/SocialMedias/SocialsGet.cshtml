@model MyAcedemyPortfolioProject.Models.Entity.TblSocialMedias

@{
    ViewBag.Title = "SocialsGet";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Edit Social Media Page</h4>
<br />
<form method="post">
    @Html.Label("ID", new { style = "visibility:hidden" })
    @Html.TextBoxFor(x => x.SocialMediaID, new { @class = "form-control", style = "visibility:hidden" })
    <br />
    @Html.Label("Social Media Name")
    @Html.TextBoxFor(x => x.SocialMediaName, new { @class = "form-control" })
    <br />
    @Html.Label("Url")
    @Html.TextBoxFor(x => x.Url, new { @class = "form-control" })
    <br />
    @Html.Label("Icon")
    @Html.TextBoxFor(x => x.Icon, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Save</button>
</form>

